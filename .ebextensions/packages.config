packages:
  yum:
    gcc-c++: []
    libxml2-devel: []
    libxslt-devel: []
    python-devel: []

sources:
  /var/fleetmap: https://github.com/fleetmap-io/odoo-fleetmap-addons/archive/refs/heads/master.zip
  /var/fieldservice: https://github.com/OCA/field-service/archive/refs/heads/13.0.zip
  /var/fleet: https://github.com/OCA/fleet/archive/refs/heads/13.0.zip
  /var/geospatial: https://github.com/OCA/geospatial/archive/refs/heads/13.0.zip

container_commands:
  fleetmap:
    command: "cp -r /var/fleetmap/odoo-fleetmap-addons-master/* ./addons"
  fieldservice:
    command: "cp -r /var/fieldservice/field-service-13.0/* ./addons"
  fleet:
    command: "cp -r /var/fleet/fleet-13.0/* ./addons"
  geospatial:
    command: "cp -r /var/geospatial/geospatial-13.0/* ./addons"
